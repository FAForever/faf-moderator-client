<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.121"
          fx:controller="com.faforever.moderatorclient.ui.TutorialAddController">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="10.0" prefWidth="130.0"/>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="300.0"/>
    </columnConstraints>
    <children>
        <Label text="Title message key"/>
        <Label text="Description key (HTML)" GridPane.rowIndex="1"/>
        <Label text="Image file name" GridPane.rowIndex="2"/>
        <Label text="Category ID" GridPane.rowIndex="3"/>
        <Label text="Ordinal" GridPane.rowIndex="4"/>
        <Label text="Launchable" GridPane.rowIndex="5"/>
        <Label text="MapVersion ID" GridPane.rowIndex="6"/>
        <Label text="Technical name" GridPane.rowIndex="7"/>
        <TextField fx:id="titleField" GridPane.columnIndex="1"/>
        <TextField fx:id="descriptionField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="imageField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="categoryField" GridPane.columnIndex="1" GridPane.rowIndex="3" disable="true"/>
        <TextField fx:id="ordinalField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <CheckBox fx:id="launchableBox" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <TextField fx:id="mapField" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <TextField fx:id="technicalNameField" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
        <Button mnemonicParsing="false" onAction="#onSave" text="Save" GridPane.columnIndex="1"
                GridPane.halignment="RIGHT" GridPane.rowIndex="9"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
