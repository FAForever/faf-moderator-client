<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.121"
          fx:controller="com.faforever.moderatorclient.ui.MessageAddController">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="10.0" prefWidth="130.0"/>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="300.0"/>
    </columnConstraints>
    <children>
        <Label text="Key"/>
        <Label text="Language" GridPane.rowIndex="1"/>
        <Label text="Region" GridPane.rowIndex="2"/>
        <Label text="Value" GridPane.rowIndex="3"/>
        <TextField fx:id="keyField" GridPane.columnIndex="1"/>
        <TextField fx:id="languageField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="regionField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="valueField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Button mnemonicParsing="false" onAction="#onSave" text="Save" GridPane.columnIndex="1"
                GridPane.halignment="RIGHT" GridPane.rowIndex="9"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
